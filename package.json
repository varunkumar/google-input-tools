{
  "name": "google-input-tools",
  "displayName": "Google Input Tools",
  "description": "Google Input Tools for Code",
  "version": "0.0.1",
  "publisher": "varunkumar",
  "author": "Varunkumar Nagarajan",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.googleInputTools",
    "*"
  ],
  "icon": "assets/images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/varunkumar/google-input-tools"
  },
  "main": "src/extension",
  "contributes": {
    "commands": [{
      "command": "extension.googleInputTools",
      "title": "Toggle Google Input Tools"
    }],
    "keybindings": [{
      "command": "extension.googleInputTools",
      "key": "ctrl+shift+i",
      "mac": "cmd+shift+i",
      "when": "editorTextFocus"
    }],
    "configuration": {
      "title": "Google Input Tools",
      "properties": {
        "google.input.tools.language": {
          "type": "string",
          "default": "ta"
        },
        "google.input.tools.suggestions": {
          "type": "integer",
          "default": 5
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0",
    "typescript": "^2.5.2",
    "vscode": "^1.1.5"
  },
  "dependencies": {
    "node-fetch": "^1.7.2"
  }
}